{
  "expo": {
    "name": "driveroute",
    "slug": "driveroute",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_azul.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.driveroute.app", 
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBa4NG_INFTFQjeebd88KzroTx_Ag8zl60"  
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
